check process directorweb-nginx with pidfile /var/vcap/sys/run/directorweb/nginx.pid
  group vcap
  start program = "/var/vcap/jobs/directorweb/bin/control-nginx start"
  stop program = "/var/vcap/jobs/directorweb/bin/control-nginx stop"
  depends on directorweb-phpfpm

check process directorweb-phpfpm with pidfile /var/vcap/sys/run/directorweb/phpfpm.pid
  group vcap
  start program = "/var/vcap/jobs/webapp/bin/control-phpfpm start"
  stop program = "/var/vcap/jobs/webapp/bin/control-phpfpm stop"

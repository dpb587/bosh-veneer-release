parameters:
    database_host: "<%= p('directorweb.db.host') %>"
    database_port: null
    database_name: "<%= p('directorweb.db.database') %>"
    database_user: "<%= p('directorweb.db.user') %>"
    database_password: "<%= p('directorweb.db.password') %>"
    database_adapter: "pdo_<%= p('directorweb.db.adapter').gsub('postgres', 'pgsql') %>"
    mailer_transport: smtp
    mailer_host: 127.0.0.1
    mailer_user: null
    mailer_password: null
    secret: ThisTokenIsNotSoSecretChangeIt

    bosh_core.director_name: <%= p('director.name') %>

    bosh_aws_cpi.region: <%= p('directorweb.aws_cpi.region') %>

    bosh_logsearch.elasticsearch.host: <%= p('directorweb.logsearch.elasticsearch.host') %>
    bosh_logsearch.elasticsearch.port: <%= p('directorweb.logsearch.elasticsearch.port') %>

    bosh_cloque.director_name: "<%= p('directorweb.cloque.director_name') %>"
    bosh_cloque.versioning.repository.workspace: "/var/vcap/store/directorweb/repository/git-root"
    bosh_cloque.versioning.repository.path_prefix: "<%= p('directorweb.cloque.versioning.repository.path_prefix') %>"
    bosh_cloque.versioning.web_service.org_name: "<%= p('directorweb.cloque.versioning.web_service.org_name') %>"
    bosh_cloque.versioning.web_service.repo_name: "<%= p('directorweb.cloque.versioning.web_service.repo_name') %>"

{% extends 'BoshCoreBundle:Deployment:layout.html.twig' %}

{% block def_title 'Packages' %}

{% block frag_content %}
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Package</th>
      </tr>
    </thead>
    <tbody>
      {% for result in results %}
        <tr>
          <td><a href="#">{{ result.name }}<span class="text-muted">/{{ result.version }}</span></a></td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
{% endblock %}

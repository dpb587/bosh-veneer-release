{% extends 'BoshCoreBundle:Release:layout.html.twig' %}

{% block def_title _bosh_core_context.package.name ~ '/' ~ _bosh_core_context.package.version %}

{% block frag_content %}  
  {#{{ render(endpoints.properties|bosh_web_appendqs({ '_frag' : 'content' })) }}#}
{% endblock %}

{% block frag_sidebar %}
  <dl>
    <dt>Packages</dt>
    {% for package in data.dependencySetJsonAsArray %}
        <dd>{{ package }}</dd>
    {% endfor %}
  </dl>
{% endblock %}

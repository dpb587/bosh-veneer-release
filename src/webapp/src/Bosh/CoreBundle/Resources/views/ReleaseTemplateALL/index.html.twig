{% extends 'BoshCoreBundle:Release:layout.html.twig' %}

{% block def_title 'Packages' %}

{% block frag_content %}
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Template</th>
        <th>Version</th>
      </tr>
    </thead>
    <tbody>
      {% for result in results %}
        <tr>
          <td><a href="">{{ result.name }}</a></td>
          <td><code>{{ result.version }}</code></td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
{% endblock %}

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_awscpi.form.type.bosheditor.deployment_diskpool" class="Veneer\AwsCpiBundle\Form\Type\BoshEditor\DeploymentDiskPoolType">
            <argument type="service" id="cpi" />

            <tag name="form.type" alias="veneer_awscpi_bosheditor_deployment_diskpool" />
        </service>

        <service id="veneer_awscpi.form.type.bosheditor.deployment_resourcepool" class="Veneer\AwsCpiBundle\Form\Type\BoshEditor\DeploymentResourcePoolType">
            <argument type="service" id="cpi" />

            <tag name="form.type" alias="veneer_awscpi_bosheditor_deployment_resourcepool" />
        </service>

        <service id="veneer_awscpi.form.type.bosheditor.deployment_resourcepool_ephemeraldisk" class="Veneer\AwsCpiBundle\Form\Type\BoshEditor\DeploymentResourcePoolEphemeralDiskType">
            <argument type="service" id="cpi" />

            <tag name="form.type" alias="veneer_awscpi_bosheditor_deployment_resourcepool_ephemeraldisk" />
        </service>
    </services>
</container>

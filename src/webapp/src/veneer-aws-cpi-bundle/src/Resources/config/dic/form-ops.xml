<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_awscpi.form.type.ops.deployment_diskpool" class="Veneer\AwsCpiBundle\Form\Type\Ops\DeploymentDiskPoolType">
            <tag name="form.type" alias="veneer_awscpi_ops_deployment_diskpool" />
        </service>

        <service id="veneer_awscpi.form.type.ops.deployment_resourcepool" class="Veneer\AwsCpiBundle\Form\Type\Ops\DeploymentResourcePoolType">
            <tag name="form.type" alias="veneer_awscpi_ops_deployment_resourcepool" />
        </service>

        <service id="veneer_awscpi.form.type.ops.deployment_network" class="Veneer\AwsCpiBundle\Form\Type\Ops\DeploymentNetworkType">
            <tag name="form.type" alias="veneer_awscpi_ops_deployment_network" />
        </service>

        <service id="veneer_awscpi.form.type.ops.deployment_resourcepool_ephemeraldisk" class="Veneer\AwsCpiBundle\Form\Type\Ops\DeploymentResourcePoolEphemeralDiskType">
            <tag name="form.type" alias="veneer_awscpi_ops_deployment_resourcepool_ephemeraldisk" />
        </service>
    </services>

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_bosh.plugin.web_request_context.builtin" class="Veneer\BoshBundle\Controller\Plugin\WebRequestContext\BuiltinPlugin">
            <argument type="service" id="doctrine.orm.bosh_entity_manager" />

            <tag name="veneer_core.request_context" context="bosh/deployment" />
            <tag name="veneer_core.request_context" context="bosh/deployment:all" />
            <tag name="veneer_core.request_context" context="bosh/deployment/property:all" />
            <tag name="veneer_core.request_context" context="bosh/deployment/property" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group:all" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance:all" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance/persistent_disk" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance/persistent_disk:all" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance/network" />
            <tag name="veneer_core.request_context" context="bosh/deployment/instance_group/instance/network:all" />
            <tag name="veneer_core.request_context" context="bosh/task" />
            <tag name="veneer_core.request_context" context="bosh/task:all" />
            <tag name="veneer_core.request_context" context="bosh/release" />
            <tag name="veneer_core.request_context" context="bosh/release:all" />
            <tag name="veneer_core.request_context" context="bosh/release/package" />
            <tag name="veneer_core.request_context" context="bosh/release/package:all" />
            <tag name="veneer_core.request_context" context="bosh/release/template" />
            <tag name="veneer_core.request_context" context="bosh/release/template:all" />
            <tag name="veneer_core.request_context" context="bosh/release/version" />
            <tag name="veneer_core.request_context" context="bosh/release/version:all" />
            <tag name="veneer_core.request_context" context="bosh/stemcell" />
            <tag name="veneer_core.request_context" context="bosh/stemcell:all" />
        </service>
    </services>
</container>

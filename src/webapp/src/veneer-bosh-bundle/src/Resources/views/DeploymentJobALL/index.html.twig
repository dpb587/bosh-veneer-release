{% extends _layout %}

{% block def_title 'Jobs' %}

{% block frag_content %}
    <table class="table table-condensed">
        <thead>
            <tr>
                <th>Job</th>
            </tr>
        </thead>
        <tbody>
            {% for result in results %}
                <tr>
                    <td><a href="{{ path('veneer_bosh_deployment_job_summary', { 'deployment' : _veneer_bosh_context.deployment.name, 'job' : result.job }) }}">{{ result.job }}</a></td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}

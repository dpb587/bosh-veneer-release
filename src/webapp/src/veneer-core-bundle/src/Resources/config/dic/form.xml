<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_core.form.type.form_picker" class="Veneer\CoreBundle\Form\Type\FormPickerType">
            <tag name="form.type" alias="veneer_core_form_picker" />
        </service>

        <service id="veneer_core.form.type.networking_cidr" class="Veneer\CoreBundle\Form\Type\NetworkingCidrType">
            <tag name="form.type" alias="veneer_core_networking_cidr" />
        </service>

        <service id="veneer_core.form.type.networking_ip" class="Veneer\CoreBundle\Form\Type\NetworkingIpType">
            <tag name="form.type" alias="veneer_core_networking_ip" />
        </service>

        <service id="veneer_core.form.type.form_picker" class="Veneer\CoreBundle\Form\Type\FormPickerType">
            <tag name="form.type" alias="veneer_core_form_picker" />
        </service>

        <service id="veneer_core.form.type.yaml" class="Veneer\CoreBundle\Form\Type\YamlType">
            <tag name="form.type" alias="veneer_core_yaml" />
        </service>

        <service id="veneer_core.form.extension.veneer" class="Veneer\CoreBundle\Form\Extension\VeneerFormExtension">
            <tag name="form.type_extension" alias="form" />
        </service>

        <service id="form.type.money" class="Veneer\CoreBundle\Form\Type\MoneyType">
            <tag name="form.type" alias="money" />
        </service>
    </services>
</container>

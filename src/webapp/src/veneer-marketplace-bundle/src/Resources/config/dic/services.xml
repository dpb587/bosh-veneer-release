<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_marketplace.marketplaces" class="Veneer\MarketplaceBundle\Service\MarketplaceFactory">
            <argument type="service" id="service_container" />
            <argument type="collection" /><!-- configuration -->
        </service>

        <service id="veneer_marketplace.updater" class="Veneer\MarketplaceBundle\Service\Updater">
            <argument type="service" id="veneer_marketplace.marketplaces" />
            <argument type="service" id="doctrine.orm.state_entity_manager" />
        </service>
    </services>
</container>

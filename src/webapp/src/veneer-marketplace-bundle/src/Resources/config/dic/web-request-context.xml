<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_marketplace.plugin.web_request_context.builtin" class="Veneer\MarketplaceBundle\Controller\Plugin\WebRequestContext\BuiltinPlugin">
            <argument type="service" id="veneer_marketplace.marketplaces" />
            <argument type="service" id="doctrine.orm.state_entity_manager" />

            <tag name="veneer_core.request_context" context="marketplace/marketplace" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/release:all" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/release" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/release/version:all" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/release/version" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/stemcell:all" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/stemcell" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/stemcell/version:all" />
            <tag name="veneer_core.request_context" context="marketplace/marketplace/stemcell/version" />
        </service>
    </services>
</container>

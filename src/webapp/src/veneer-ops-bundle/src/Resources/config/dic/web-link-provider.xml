<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_ops.plugin.web_link_provider.widget" class="Veneer\OpsBundle\Controller\Plugin\WebLinkProvider\WidgetPlugin">
            <argument type="service" id="doctrine.orm.state_entity_manager" />

            <tag name="veneer_core.link_provider" route="veneer_bosh_deployment_summary" />
            <tag name="veneer_core.link_provider" route="veneer_bosh_deployment_job_summary" />
            <tag name="veneer_core.link_provider" route="veneer_bosh_deployment_job_index_summary" />
        </service>
    </services>
</container>

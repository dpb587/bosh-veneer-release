<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="veneer_sheaf.plugin.web_workspace_app.sheaf" class="Veneer\SheafBundle\Service\Workspace\App\SheafApp">
            <argument type="service" id="veneer_ops.manifest_builder.bosh" />
            <argument type="service" id="veneer_bosh.api" />
            <argument type="service" id="logger" />

            <tag name="veneer_core.link_provider" route="veneer_bosh_cloudconfig_summary" />
            <tag name="veneer_core.workspace.app" path=";bosh/cloud-config/manifest.yml;" />
            <tag name="monolog.logger" channel="veneer_sheaf.workspace.app.sheaf" />
        </service>
    </services>
</container>
